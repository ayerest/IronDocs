<!DOCTYPE html>
<html>
  <head>
    <title>IronDocsApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <nav>
      <ul class="nav">
          <li>
            <%= link_to "Log out", logout_path, method: :delete %>
          </li>
      </ul>
    </nav>
    <%= yield %>
  </body>
</html>
